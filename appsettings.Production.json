{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "UrlDomain": "www.api.zetko.dev",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:80"
      },
      "HttpsFromPem": {
        "Url": "https://0.0.0.0:443",
        "Certificate": {
          "Path": "/app/certificates/fullchain.pem",
          "KeyPath": "/app/certificates/privkey.pem"
        }
      }
    }
  }
}
